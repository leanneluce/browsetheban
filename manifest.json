{
  "manifest_version": 2,
  "name": "Browse the Ban",
  "version": "0.1",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "exclude_matches": [
        "https://*.google.com/*"
      ],
      "js": ["content.js"],
      "css": ["browsetheban.css"]
    }
  ]
}
